package com.awl.tch.brandemi.model;

public class ValidationParameter {
	private String validationType;
	private String numberOfDays;
	private String velocityType;
	private String velocityCount;
	private String velocityNumberOfDays;
	private boolean velocityCheck;
	
	
	public ValidationParameter(String validationType, String numberOfDays, String velocityType, String velocityCount,
			String velocityNumberOfDays, boolean velocityCheck) {
		super();
		this.validationType = validationType;
		this.numberOfDays = numberOfDays;
		this.velocityType = velocityType;
		this.velocityCount = velocityCount;
		this.velocityNumberOfDays = velocityNumberOfDays;
		this.velocityCheck = velocityCheck;
	}
	public String getVelocityType() {
		return velocityType;
	}
	public void setVelocityType(String velocityType) {
		this.velocityType = velocityType;
	}
	public String getValidationType() {
		return validationType;
	}
	public void setValidationType(String validationType) {
		this.validationType = validationType;
	}
	public String getNumberOfDays() {
		return numberOfDays;
	}
	public void setNumberOfDays(String numberOfDays) {
		this.numberOfDays = numberOfDays;
	}
	public String getVelocityCount() {
		return velocityCount;
	}
	public void setVelocityCount(String velocityCount) {
		this.velocityCount = velocityCount;
	}
	public String getVelocityNumberOfDays() {
		return velocityNumberOfDays;
	}
	public void setVelocityNumberOfDays(String velocityNumberOfDays) {
		this.velocityNumberOfDays = velocityNumberOfDays;
	}
	public boolean isVelocityCheck() {
		return velocityCheck;
	}
	public void setVelocityCheck(boolean velocityCheck) {
		this.velocityCheck = velocityCheck;
	}
	@Override
	public String toString() {
		return "ValidationParameter [validationType=" + validationType + ", numberOfDays=" + numberOfDays
				+ ", velocityType=" + velocityType + ", velocityCount=" + velocityCount + ", velocityNumberOfDays="
				+ velocityNumberOfDays + "]";
	}
	
	
}
