package com.awl.tch.model;

import com.awl.tch.annotation.Column;
import com.awl.tch.annotation.Id;
import com.awl.tch.annotation.Table;

@Table(name = "TCH_UTILITY_CONFIG")
public class UtilityConfigDTO {

	@Column(name="U_ID")
	@Id
	private long uid;
	
	@Column(name="U_KEY")
	private String ukey;
	
	@Column(name="U_VALUE")
	private String uvalue;
		
	@Column(name="U_UTILITY_NAME")
	private String utiltyName;

	/**
	 * @return the uid
	 */
	public long getUid() {
		return uid;
	}

	/**
	 * @param uid the uid to set
	 */
	public void setUid(long uid) {
		this.uid = uid;
	}

	/**
	 * @return the ukey
	 */
	public String getUkey() {
		return ukey;
	}

	/**
	 * @param ukey the ukey to set
	 */
	public void setUkey(String ukey) {
		this.ukey = ukey;
	}

	/**
	 * @return the uvalue
	 */
	public String getUvalue() {
		return uvalue;
	}

	/**
	 * @param uvalue the uvalue to set
	 */
	public void setUvalue(String uvalue) {
		this.uvalue = uvalue;
	}

	/**
	 * @return the utiltyName
	 */
	public String getUtiltyName() {
		return utiltyName;
	}

	/**
	 * @param utiltyName the utiltyName to set
	 */
	public void setUtiltyName(String utiltyName) {
		this.utiltyName = utiltyName;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "UtilityConfigDTO [ukey=" + ukey + ", uvalue=" + uvalue
				+ ", utiltyName=" + utiltyName + "]";
	}

	
}
