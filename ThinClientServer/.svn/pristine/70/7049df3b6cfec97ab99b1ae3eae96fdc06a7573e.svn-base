package com.awl.job.tch.schedular.model;

import com.awl.tch.annotation.Column;
import com.awl.tch.annotation.Id;
import com.awl.tch.annotation.Table;

@Table(name = "TCH_JOB_INFO")
public class JobEntityDTO {

	@Id
	@Column(name = "J_ID")
	private Long id;
	
	@Column(name = "J_JOB_NAME")
	private String jobName;
	
	@Column(name = "J_GROUP_NAME")
	private String jobGruopName;
	
	@Column(name = "J_JOB_CLASS_NAME")
	private String jobClassName;
	
	@Column(name = "J_JOB_CRON_EXPR")
	private String jobCronExpression;
	
	@Column(name = "J_JOB_STATUS")
	private String jobStatus;
	
	@Column(name = "J_IDENTITY_NAME")
	private String jobIdentityName;
	
	
	@Column(name = "J_JOB_DESCRIPTION")
	private String jobDescription;

	
	
	/**
	 * @return the jobDescription
	 */
	public String getJobDescription() {
		return jobDescription;
	}

	/**
	 * @param jobDescription the jobDescription to set
	 */
	public void setJobDescription(String jobDescription) {
		this.jobDescription = jobDescription;
	}

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the jobName
	 */
	public String getJobName() {
		return jobName;
	}

	/**
	 * @param jobName the jobName to set
	 */
	public void setJobName(String jobName) {
		this.jobName = jobName;
	}

	/**
	 * @return the jobGruopName
	 */
	public String getJobGruopName() {
		return jobGruopName;
	}

	/**
	 * @param jobGruopName the jobGruopName to set
	 */
	public void setJobGruopName(String jobGruopName) {
		this.jobGruopName = jobGruopName;
	}

	/**
	 * @return the jobClassName
	 */
	public String getJobClassName() {
		return jobClassName;
	}

	/**
	 * @param jobClassName the jobClassName to set
	 */
	public void setJobClassName(String jobClassName) {
		this.jobClassName = jobClassName;
	}

	/**
	 * @return the jobCronExpression
	 */
	public String getJobCronExpression() {
		return jobCronExpression;
	}

	/**
	 * @param jobCronExpression the jobCronExpression to set
	 */
	public void setJobCronExpression(String jobCronExpression) {
		this.jobCronExpression = jobCronExpression;
	}

	/**
	 * @return the jobStatus
	 */
	public String getJobStatus() {
		return jobStatus;
	}

	/**
	 * @param jobStatus the jobStatus to set
	 */
	public void setJobStatus(String jobStatus) {
		this.jobStatus = jobStatus;
	}

	/**
	 * @return the jobIdentityName
	 */
	public String getJobIdentityName() {
		return jobIdentityName;
	}

	/**
	 * @param jobIdentityName the jobIdentityName to set
	 */
	public void setJobIdentityName(String jobIdentityName) {
		this.jobIdentityName = jobIdentityName;
	}
	
	
	
	
	
	
	
	
}
