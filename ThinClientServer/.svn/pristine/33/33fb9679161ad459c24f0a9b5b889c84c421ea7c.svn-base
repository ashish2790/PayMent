package com.awl.tch.bean;

import com.google.gson.annotations.SerializedName;

public class FirstMenu {

	
	@SerializedName("tbN")
	private String tableNumberString;
	
	@SerializedName("cTCd")
	private String currentTableCodeString;
	
	@SerializedName("nTCd")
	private String nextTableCodeString;
	
	@SerializedName("dspNm")
	private String displayName;
	
	@SerializedName("hdNm")
	private String headerName;
	
	@SerializedName("ctId")
	private String categoryIdString="0";
	
	private transient String value;
	private transient String menuFlag;
	private transient boolean leaf;
	private transient Integer rootId;
	/* (non-Javadoc)
	 * @see java.lang.Object#hashCode()
	 */
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime
				* result
				+ ((categoryIdString == null) ? 0 : categoryIdString.hashCode());
		result = prime
				* result
				+ ((currentTableCodeString == null) ? 0
						: currentTableCodeString.hashCode());
		result = prime * result
				+ ((displayName == null) ? 0 : displayName.hashCode());
		result = prime * result
				+ ((headerName == null) ? 0 : headerName.hashCode());
		result = prime
				* result
				+ ((nextTableCodeString == null) ? 0 : nextTableCodeString
						.hashCode());
		result = prime
				* result
				+ ((tableNumberString == null) ? 0 : tableNumberString
						.hashCode());
		return result;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		FirstMenu other = (FirstMenu) obj;
		if (categoryIdString == null) {
			if (other.categoryIdString != null)
				return false;
		} else if (!categoryIdString.equals(other.categoryIdString))
			return false;
		if (currentTableCodeString == null) {
			if (other.currentTableCodeString != null)
				return false;
		} else if (!currentTableCodeString.equals(other.currentTableCodeString))
			return false;
		if (displayName == null) {
			if (other.displayName != null)
				return false;
		} else if (!displayName.equals(other.displayName))
			return false;
		if (headerName == null) {
			if (other.headerName != null)
				return false;
		} else if (!headerName.equals(other.headerName))
			return false;
		if (nextTableCodeString == null) {
			if (other.nextTableCodeString != null)
				return false;
		} else if (!nextTableCodeString.equals(other.nextTableCodeString))
			return false;
		if (tableNumberString == null) {
			if (other.tableNumberString != null)
				return false;
		} else if (!tableNumberString.equals(other.tableNumberString))
			return false;
		return true;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "FirstMenu [tableNumberString=" + tableNumberString
				+ ", currentTableCodeString=" + currentTableCodeString
				+ ", nextTableCodeString=" + nextTableCodeString
				+ ", displayName=" + displayName + ", headerName=" + headerName
				+ ", categoryIdString=" + categoryIdString + "]";
	}
	/**
	 * @return the tableNumberString
	 */
	public String getTableNumberString() {
		return tableNumberString;
	}
	/**
	 * @param tableNumberString the tableNumberString to set
	 */
	public void setTableNumberString(String tableNumberString) {
		this.tableNumberString = tableNumberString;
	}
	/**
	 * @return the currentTableCodeString
	 */
	public String getCurrentTableCodeString() {
		return currentTableCodeString;
	}
	/**
	 * @param currentTableCodeString the currentTableCodeString to set
	 */
	public void setCurrentTableCodeString(String currentTableCodeString) {
		this.currentTableCodeString = currentTableCodeString;
	}
	/**
	 * @return the nextTableCodeString
	 */
	public String getNextTableCodeString() {
		return nextTableCodeString;
	}
	/**
	 * @param nextTableCodeString the nextTableCodeString to set
	 */
	public void setNextTableCodeString(String nextTableCodeString) {
		this.nextTableCodeString = nextTableCodeString;
	}
	/**
	 * @return the displayName
	 */
	public String getDisplayName() {
		return displayName;
	}
	/**
	 * @param displayName the displayName to set
	 */
	public void setDisplayName(String displayName) {
		this.displayName = displayName;
	}
	/**
	 * @return the headerName
	 */
	public String getHeaderName() {
		return headerName;
	}
	/**
	 * @param headerName the headerName to set
	 */
	public void setHeaderName(String headerName) {
		this.headerName = headerName;
	}
	/**
	 * @return the categoryIdString
	 */
	public String getCategoryIdString() {
		return categoryIdString;
	}
	/**
	 * @param categoryIdString the categoryIdString to set
	 */
	public void setCategoryIdString(String categoryIdString) {
		this.categoryIdString = categoryIdString;
	}
	/**
	 * @return the value
	 */
	public String getValue() {
		return value;
	}
	/**
	 * @param value the value to set
	 */
	public void setValue(String value) {
		this.value = value;
	}
	/**
	 * @return the menuFlag
	 */
	public String getMenuFlag() {
		return menuFlag;
	}
	/**
	 * @param menuFlag the menuFlag to set
	 */
	public void setMenuFlag(String menuFlag) {
		this.menuFlag = menuFlag;
	}
	/**
	 * @return the leaf
	 */
	public boolean isLeaf() {
		return leaf;
	}
	/**
	 * @param leaf the leaf to set
	 */
	public void setLeaf(boolean leaf) {
		this.leaf = leaf;
	}
	/**
	 * @return the rootId
	 */
	public Integer getRootId() {
		return rootId;
	}
	/**
	 * @param rootId the rootId to set
	 */
	public void setRootId(Integer rootId) {
		this.rootId = rootId;
	}
	
}
